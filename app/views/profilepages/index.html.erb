
<h1>Listing Profilepages</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>String,</th>
      <th>Artist</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profilepages.each do |profilepage| %>
      <tr>
        <td><%= profilepage.name %></td>
        <td><%= profilepage.description %></td>
        <td><%= profilepage.string %></td>
        <td><%= profilepage.user.name %></td>
        <td><%= link_to 'Show', profilepage %></td>
        <td><%= link_to 'Edit', edit_profilepage_path(profilepage) %></td>
        <td><%= link_to 'Destroy', profilepage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profilepage', new_profilepage_path %>
